  ALTER TABLE [addressBook] ADD CONSTRAINT UNIQ_PAN_ADDRESS_CITY UNIQUE ([pan],[address1],[address2],[city]);
  ALTER TABLE [addressBook] ADD CONSTRAINT UNIQ_PAN_NAME UNIQUE ([pan],[name]);

  create unique nonclustered index NCI_UNIQ_PAN_isWork
  on [addressBook] ([isWork])
    include ([pan]) 
  where [isWork]=1


  create unique nonclustered index NCI_UNIQ_PAN_isHome
  on [addressBook] ([isHome])
    include ([pan]) 
  where [isHome]=1

    create unique nonclustered index NCI_UNIQ_PAN_isDefault
  on [addressBook] ([isDefault])
    include ([pan]) 
  where [isDefault]=1